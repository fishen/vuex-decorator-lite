{
  "name": "vuex-decorator-lite",
  "version": "0.0.3",
  "description": "A vuex proxy used with class",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.{ts,tsx}",
    "ts": "ts-node",
    "demo": "ts-node test/demo",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check&&npm run fix&&webpack",
    "pack": "npm run build&&npm pack"
  },
  "keywords": [
    "vuex",
    "vue"
  ],
  "author": "fisher <i.yu@qq.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fishen/vuex-model.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "bundle-dts": "^1.1.4",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "vuex": "^3.5.1"
  }
}
